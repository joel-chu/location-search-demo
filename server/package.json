{
  "private": true,
  "name": "demo-server",
  "version": "0.0.1",
  "description": "location search demo",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:m": "ava --verbose ./tests/model.test.ts",
    "test:cli": "ava --verbose ./tests/cli.test.ts",
    "test:srv": "ava --verbose ./tests/server.test.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build": "esbuild",
    "clean": "node ./clean.js",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "serve": "npm run ts -- src/server.ts",
    "cli": "npm run ts -- ./cli.ts",
    "start": "nodemon",
    "ts": "node -r esbuild-register",
    "ormcli": "npm run ts -- ./node_modules/.bin/typeorm"
  },
  "keywords": [
    "location",
    "search"
  ],
  "author": "Joel Chu <joelckchu@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "@koa/router": "^10.0.0",
    "fast-csv": "^4.3.6",
    "fs-extra": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "marked": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "tslib": "^1.14.1",
    "typeorm": "^0.2.32",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/fs-extra": "^9.0.11",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.2",
    "@types/marked": "^2.0.3",
    "@types/node": "^15.6.1",
    "@types/sqlite3": "^3.1.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "ava": "^3.15.0",
    "esbuild": "^0.12.0",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.27.0",
    "nodemon": "^2.0.7",
    "supertest": "^1.2.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  }
}
